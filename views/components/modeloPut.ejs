<section class="modelo" id="go">
    <div class="model" id="model">
        <div class="model-header">
            <h4>ATUALIZAR POKEMON</h4>
            <a href="/">X</a>

        </div>
        <div class="model-body">
            <form action="/update/<% pokemonUp.id %>" method="POST">

                <label for="nome">Nome: </label>
                <input type="text" id="nome" name="nome" value="<% pokemonUp.nome %>">
    
                <label for="descricao">Descrição: </label>
                <input type="text" id="descricao" name="descricao" value="<% pokemonUp.descricao %>>
    
                <label for="tipo">Tipo: </label>
                <input type="text" id="tipo" name="tipo" value="<% pokemonUp.tipo%>>
    
                <label for="imagem">Link da imagem: </label>
                <input type="text" id="imagem" name="imagem" value="<% pokemonUp.imagem %>>
    
                <button id = "btnCadastro" type="submit">ATUALIZAR</button>
            </form>
        </div>
    </div>
</section>